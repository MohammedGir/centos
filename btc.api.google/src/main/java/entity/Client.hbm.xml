<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="entity.Client" table="client">
        <id name="id">
            <generator class="assigned"></generator>
        </id>

        <property name="apellId"></property>
        <property name="commerial"></property>
        <property name="joignable"></property>
        <property name="nature"></property>
        <property name="createdDate"></property>
        <property name="qualification"></property>


    </class>

</hibernate-mapping>